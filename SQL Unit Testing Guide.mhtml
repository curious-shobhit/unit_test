from pyspark.sql import SparkSession

# Create SparkSession
spark =3D SparkSession.builder \
          .appName('SparkByExamples.com') \
          .getOrCreate()
#EMP DataFrame
empData =3D [(1,"Smith",10), (2,"Rose",20),
    (3,"Williams",10), (4,"Jones",30)
  ]
empColumns =3D ["emp_id","name","emp_dept_id"]
empDF =3D spark.createDataFrame(empData,empColumns)
empDF.show()

#DEPT DataFrame
deptData =3D [("Finance",10), ("Marketing",20),
    ("Sales",30),("IT",40)
  ]
deptColumns =3D ["dept_name","dept_id"]
deptDF=3Dspark.createDataFrame(deptData,deptColumns) =20
deptDF.show()

#Address DataFrame
addData=3D[(1,"1523 Main St","SFO","CA"),
    (2,"3453 Orange St","SFO","NY"),
    (3,"34 Warner St","Jersey","NJ"),
    (4,"221 Cavalier St","Newark","DE"),
    (5,"789 Walnut St","Sandiago","CA")
  ]
addColumns =3D ["emp_id","addline1","city","state"]
addDF =3D spark.createDataFrame(addData,addColumns)
addDF.show()

#Join two DataFrames
empDF.join(addDF,empDF["emp_id"] =3D=3D addDF["emp_id"]).show()

#Drop duplicate column
empDF.join(addDF,["emp_id"]).show()

#Join Multiple DataFrames
empDF.join(addDF,["emp_id"]) \
     .join(deptDF,empDF["emp_dept_id"] =3D=3D deptDF["dept_id"]) \
     .show()

#Using Where for Join Condition
empDF.join(deptDF).where(empDF["emp_dept_id"] =3D=3D deptDF["dept_id"]) \
    .join(addDF).where(empDF["emp_id"] =3D=3D addDF["emp_id"]) \
    .show()
   =20
#SQL
empDF.createOrReplaceTempView("EMP")
deptDF.createOrReplaceTempView("DEPT")
addDF.createOrReplaceTempView("ADD")

spark.sql("select * from EMP e, DEPT d, ADD a " + \
    "where e.emp_dept_id =3D=3D d.dept_id and e.emp_id =3D=3D a.emp_id") \
    .show()
